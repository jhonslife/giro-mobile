#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jhonslife/giro-mobile/node_modules/.pnpm/react-native@0.76.9_@babel+core@7.28.5_@babel+preset-env@7.28.5_@babel+core@7.28.5__@types+react@18.3.27_react@18.3.1/node_modules/react-native/node_modules:/home/jhonslife/giro-mobile/node_modules/.pnpm/react-native@0.76.9_@babel+core@7.28.5_@babel+preset-env@7.28.5_@babel+core@7.28.5__@types+react@18.3.27_react@18.3.1/node_modules:/home/jhonslife/giro-mobile/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/jhonslife/giro-mobile/node_modules/.pnpm/react-native@0.76.9_@babel+core@7.28.5_@babel+preset-env@7.28.5_@babel+core@7.28.5__@types+react@18.3.27_react@18.3.1/node_modules/react-native/node_modules:/home/jhonslife/giro-mobile/node_modules/.pnpm/react-native@0.76.9_@babel+core@7.28.5_@babel+preset-env@7.28.5_@babel+core@7.28.5__@types+react@18.3.27_react@18.3.1/node_modules:/home/jhonslife/giro-mobile/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/react-native@0.76.9_@babel+core@7.28.5_@babel+preset-env@7.28.5_@babel+core@7.28.5__@types+react@18.3.27_react@18.3.1/node_modules/react-native/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/react-native@0.76.9_@babel+core@7.28.5_@babel+preset-env@7.28.5_@babel+core@7.28.5__@types+react@18.3.27_react@18.3.1/node_modules/react-native/cli.js" "$@"
fi
