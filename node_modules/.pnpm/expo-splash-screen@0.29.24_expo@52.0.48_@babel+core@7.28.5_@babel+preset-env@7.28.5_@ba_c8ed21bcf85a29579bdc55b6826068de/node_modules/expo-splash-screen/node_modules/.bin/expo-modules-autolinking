#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jhonslife/giro-mobile/node_modules/.pnpm/expo@52.0.48_@babel+core@7.28.5_@babel+preset-env@7.28.5_@babel+core@7.28.5__@expo+metr_a1fba041f4bec05ce3628270c0a8bada/node_modules/expo/bin/node_modules:/home/jhonslife/giro-mobile/node_modules/.pnpm/expo@52.0.48_@babel+core@7.28.5_@babel+preset-env@7.28.5_@babel+core@7.28.5__@expo+metr_a1fba041f4bec05ce3628270c0a8bada/node_modules/expo/node_modules:/home/jhonslife/giro-mobile/node_modules/.pnpm/expo@52.0.48_@babel+core@7.28.5_@babel+preset-env@7.28.5_@babel+core@7.28.5__@expo+metr_a1fba041f4bec05ce3628270c0a8bada/node_modules:/home/jhonslife/giro-mobile/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/jhonslife/giro-mobile/node_modules/.pnpm/expo@52.0.48_@babel+core@7.28.5_@babel+preset-env@7.28.5_@babel+core@7.28.5__@expo+metr_a1fba041f4bec05ce3628270c0a8bada/node_modules/expo/bin/node_modules:/home/jhonslife/giro-mobile/node_modules/.pnpm/expo@52.0.48_@babel+core@7.28.5_@babel+preset-env@7.28.5_@babel+core@7.28.5__@expo+metr_a1fba041f4bec05ce3628270c0a8bada/node_modules/expo/node_modules:/home/jhonslife/giro-mobile/node_modules/.pnpm/expo@52.0.48_@babel+core@7.28.5_@babel+preset-env@7.28.5_@babel+core@7.28.5__@expo+metr_a1fba041f4bec05ce3628270c0a8bada/node_modules:/home/jhonslife/giro-mobile/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../expo@52.0.48_@babel+core@7.28.5_@babel+preset-env@7.28.5_@babel+core@7.28.5__@expo+metr_a1fba041f4bec05ce3628270c0a8bada/node_modules/expo/bin/autolinking" "$@"
else
  exec node  "$basedir/../../../../../expo@52.0.48_@babel+core@7.28.5_@babel+preset-env@7.28.5_@babel+core@7.28.5__@expo+metr_a1fba041f4bec05ce3628270c0a8bada/node_modules/expo/bin/autolinking" "$@"
fi
